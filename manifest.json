{
    "name": "Base extension",
    "description": "Base Google Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "function.js"
    },
    "permissions": [
        "tabs", "storage", "activeTab", "scripting", "background"
    ],
    "host_permissions": [
        "*://*/"
    ],
    "action": {
        "default_popup": "interface.html",
        "default_icon": {
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}